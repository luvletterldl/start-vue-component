{
  "name": "doc",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.19.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 54321 --open",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@vue-component/ui": "workspace:*",
    "@vueuse/core": "catalog:",
    "vue": "catalog:",
    "vue-router": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@types/node": "catalog:",
    "@unocss/reset": "catalog:",
    "@unocss/transformer-directives": "catalog:",
    "@unocss/transformer-variant-group": "catalog:",
    "@vitejs/plugin-basic-ssl": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vue/test-utils": "catalog:",
    "eslint": "catalog:",
    "jsdom": "catalog:",
    "lightningcss": "catalog:",
    "lint-staged": "catalog:",
    "pnpm": "catalog:",
    "simple-git-hooks": "catalog:",
    "taze": "catalog:",
    "terser": "catalog:",
    "typescript": "catalog:",
    "unocss": "catalog:",
    "unplugin-auto-import": "catalog:",
    "unplugin-icons": "catalog:",
    "unplugin-vue-components": "catalog:",
    "unplugin-vue-inspector": "catalog:",
    "vite": "catalog:",
    "vite-plugin-pages": "catalog:",
    "vite-plugin-vue-devtools": "catalog:",
    "vitest": "catalog:",
    "vue-tsc": "catalog:"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
