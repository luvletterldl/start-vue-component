{
  "name": "doc",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 54321 --open",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@vue-component/ui": "workspace:*",
    "@vueuse/core": "^13.2.0",
    "vue": "^3.5.14",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@types/node": "^22.15.21",
    "@unocss/reset": "^66.1.2",
    "@unocss/transformer-directives": "^66.1.2",
    "@unocss/transformer-variant-group": "^66.1.2",
    "@vitejs/plugin-basic-ssl": "^2.0.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.27.0",
    "jsdom": "^26.1.0",
    "lightningcss": "^1.30.1",
    "lint-staged": "^16.0.0",
    "pnpm": "^10.11.0",
    "simple-git-hooks": "^2.13.0",
    "taze": "^19.1.0",
    "terser": "^5.39.2",
    "typescript": "^5.8.3",
    "unocss": "^66.1.2",
    "unplugin-auto-import": "^19.2.0",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.5.0",
    "unplugin-vue-inspector": "^2.3.1",
    "vite": "^6.3.5",
    "vite-plugin-pages": "^0.33.0",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vitest": "^3.1.4",
    "vue-tsc": "^2.2.10"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
