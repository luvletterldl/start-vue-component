{
  "name": "@vue-component/ui",
  "type": "module",
  "version": "0.0.0",
  "author": "lidongliang01",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://.git",
    "directory": "packages/ui"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ui.js"
    }
  },
  "main": "dist/ui.js",
  "module": "dist/ui.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "dev:build": "vite build && tsc -d --emitDeclarationOnly && vue-typegen gen -s ./src -o ./dist",
    "lint": "eslint .",
    "format": "eslint . --fix"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "publishConfig": {
    "access": "public",
    "registry": ""
  },
  "devDependencies": {
    "@babel/types": "catalog:",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vue/compiler-sfc": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-css-injected-by-js": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vue": "catalog:",
    "vue-tsc": "catalog:",
    "vue-typegen": "catalog:"
  }
}
